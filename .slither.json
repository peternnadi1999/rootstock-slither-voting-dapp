{
  "detectors": {
    "exclude": [
      "solc-version",
      "naming-convention",
      "unused-state"
    ],
    "include": [
      "reentrancy",
      "access-control",
      "missing-zero-check",
      "uninitialized-state"
    ]
  },
  "filter_paths": [
    "node_modules",
    "test"
  ],
  "solc_remaps": [],
  "solc_args": [
    "--optimize",
    "--optimize-runs",
    "200"
  ],
  "output": {
    "json": "slither-report.json",
    "sarif": "slither-report.sarif"
  },
  "exclude_informational": true,
  "exclude_low": true,
  "exclude_medium": false,
  "exclude_high": false
}